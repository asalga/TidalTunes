bps(100/60)

d1  $fast 2 $ sound "db*4"

d2 $ palindrome $ sound "hit ~ hit:2 hit:3?"

-- scramble n p
-- divide pattern p into n parts
d3 $fast 1 $ sound $ scramble 4 "~ arpy db arpy:6"

d4 $ sound "hit:3 ~ arpy:3" # gain 0.5

d5 $slow 8 $ sound "alphabet:14*3" #gain 0.5

d1 silence

d2 silence

d3 silence

d4 silence

hush
